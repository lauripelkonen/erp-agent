{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3LearningBucketAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::offer-learning-data",
        "arn:aws:s3:::offer-learning-data/*"
      ]
    }
  ]
}
